{
  "header": 
    {
      "html": '
      
      <header class="header" appFixedHeader offset="150">
    <div class="container d-flex">

        <!--header__firstSide-->
        <div class="header__firstSide">
          <a routerLink="/{{language['LANGUAGE_CODE']}}" class="header__logo">
            <img *ngIf="logo$ | async as logo" [lazyLoad]="logo" alt="websiteLogo">
          </a>
        </div>
        <!--header__firstSide-->

        <!--header__secondSide-->
        <div class="header__secondSide">

              <!--header__top-->
              <div class="header__top">
                    <!--header__userLinks-->
                    <div class="header__userLinks" appClosest>
                        <button class="custom-button custom-button--nested">
                            <svg class="svg-icon" viewBox="0 0 20 20">
                                <path d="M10,10.9c2.373,0,4.303-1.932,4.303-4.306c0-2.372-1.93-4.302-4.303-4.302S5.696,4.223,5.696,6.594C5.696,8.969,7.627,10.9,10,10.9z M10,3.331c1.801,0,3.266,1.463,3.266,3.263c0,1.802-1.465,3.267-3.266,3.267c-1.8,0-3.265-1.465-3.265-3.267C6.735,4.794,8.2,3.331,10,3.331z"></path>
                                <path  d="M10,12.503c-4.418,0-7.878,2.058-7.878,4.685c0,0.288,0.231,0.52,0.52,0.52c0.287,0,0.519-0.231,0.519-0.52c0-1.976,3.132-3.646,6.84-3.646c3.707,0,6.838,1.671,6.838,3.646c0,0.288,0.234,0.52,0.521,0.52s0.52-0.231,0.52-0.52C17.879,14.561,14.418,12.503,10,12.503z"></path>
                            </svg>
                        </button>
                        <ul class="userMenu">

                            <li class="userMenu__userList" *ngIf="wishListFeature$ | async">
                                <a  routerLink="/{{language['LANGUAGE_CODE']}}/wish-list"
                                 class="userMenu__userLink">{{language['wishList']}}</a>
                            </li>
                            
                            <li class="userMenu__userList" *ngIf="compareFeature$ | async">
                                <a 
                                routerLink="/{{language['LANGUAGE_CODE']}}/compare" 
                                class="userMenu__userLink">{{language['compare']}}</a>
                            </li>

                            <ng-container *ngIf="logged$ | async; else login">
                                <li class="userMenu__userList">
                                    <a [routerLink]="['/', language['LANGUAGE_CODE'], 'profile', 'dashboard']"
                                     class="userMenu__userLink">{{userName | async}}</a>
                                </li>
                                <li class="userMenu__userList">
                                    <a [routerLink]="['/accounts/login']" (click)="logout()"
                                     class="userMenu__userLink">{{language['logout']}}</a>
                                </li>
                            </ng-container>
                            
                            <ng-template #login>

                            <li class="userMenu__userList">
                                <a routerLink="/{{language['LANGUAGE_CODE']}}/accounts/login"
                                 class="userMenu__userLink">{{language['login']}}</a>
                            </li>
                            <li class="userMenu__userList">
                                <a routerLink="/{{language['LANGUAGE_CODE']}}/accounts/sign-up" class="userMenu__userLink">{{language['register']}}</a>
                            </li>

                            </ng-template>


                        </ul>
                    </div>
                    <!--header__userLinks-->

                    <!--header__search-->
                   <app-search></app-search>
                    <!--header__search-->
              </div>
              <!--header__top-->

              <!--main navigation-->
              <div class="header__mainNavigation">

                <button class="custom-button custom-button--responsiveButton" (click)="openSide($event);$event.stopPropagation();" #sideButton>
                    <svg class="svg-icon" viewBox="0 0 20 20">
                        <path d="M3.314,4.8h13.372c0.41,0,0.743-0.333,0.743-0.743c0-0.41-0.333-0.743-0.743-0.743H3.314
                            c-0.41,0-0.743,0.333-0.743,0.743C2.571,4.467,2.904,4.8,3.314,4.8z M16.686,15.2H3.314c-0.41,0-0.743,0.333-0.743,0.743
                            s0.333,0.743,0.743,0.743h13.372c0.41,0,0.743-0.333,0.743-0.743S17.096,15.2,16.686,15.2z M16.686,9.257H3.314
                            c-0.41,0-0.743,0.333-0.743,0.743s0.333,0.743,0.743,0.743h13.372c0.41,0,0.743-0.333,0.743-0.743S17.096,9.257,16.686,9.257z"></path>
                    </svg>
                </button>

                <!--blackLayer-->
                <div class="blackLayer" [ngClass]="{'active' : openSideMenu}" (click)="openSide($event);$event.stopPropagation();" #sideLayer>
                    <div class="menu-container">
                        <!--primary menu-->
                        <ul class="header__primaryMenu">

                            <li class="header__primaryMenu__list has-child"  appToggleActiveSame >
                                <a (click)="openSide($event);$event.stopPropagation();"
                                      routerLink="/{{language['LANGUAGE_CODE']}}/collection" 
                                      class="header__primaryMenu__link">
                                {{language['allCategories']}}
                                </a>

                                <ng-container *ngIf="taxons?.length">
                                    <!--drop down first level-->
                                    <ul class="dropdown">
                                        <ng-container *ngFor="let taxon of taxons">

                                            <!--taxon  have  children-->
                                            <ng-container *ngIf="taxon.children">
                                                <li class="dropdown__item has-child " appToggleActiveSame>
                                                    <a 
                                                         (click)="openSide($event);$event.stopPropagation();"
                                                        routerLink="/{{language['LANGUAGE_CODE']}}/collection"
                                                        [queryParams]="{taxons: taxon.code}" class="dropdown__link"
                                                        (click)="$event.stopPropagation()"
                                                        >
                                                        {{taxon | translate: 'name'}}
                                                    </a>

                                                    <!--drop down second level-->
                                                    <ul class="dropdown">
                                                            <ng-container *ngFor="let child of taxon.children">

                                                                <!--if child have a child third level-->
                                                                <ng-container *ngIf="child.children?.length">
                                                                    <li class="dropdown__item has-child" appToggleActiveSame>
                                                                        <a (click)="openSide($event);$event.stopPropagation();" routerLink="/{{language['LANGUAGE_CODE']}}/collection" (click)="$event.stopPropagation()"
                                                                        [queryParams]="{taxons: child.code}" class="dropdown__link">{{child | translate: 'name'}}</a>
                                                                        <ul class="dropdown">
                                                                            <li class="dropdown__item" *ngFor="let child of child.children">
                                                                                <a (click)="openSide($event);$event.stopPropagation();" routerLink="/{{language['LANGUAGE_CODE']}}/collection" (click)="$event.stopPropagation()"
                                                                                [queryParams]="{taxons: child.code}" class="dropdown__link">{{child | translate: 'name'}}</a>
                                                                            </li>
                                                                        </ul>
                                                                    </li>
                                                                </ng-container>
                                                                <!--if child have a child level-->

                                                                <!--if child don't have a child-->
                                                                <ng-container *ngIf="!child.children?.length">
                                                                    <li class="dropdown__item">
                                                                        <a (click)="openSide($event);$event.stopPropagation();" routerLink="/{{language['LANGUAGE_CODE']}}/collection" (click)="$event.stopPropagation()"
                                                                        [queryParams]="{taxons: child.code}" class="dropdown__link">{{child | translate: 'name'}}</a>
                                                                    </li>
                                                                </ng-container>
                                                                <!--if child don't have a child-->
                                                                
                                                            </ng-container>
                                                           
                                                    </ul>
                                                    <!--drop down second level-->

                                                </li>
                                            </ng-container>
                                            <!--taxon  have  children-->


                                            <!--taxon don't have any children-->
                                            <ng-container *ngIf="!taxon.children">
                                                <li class="dropdown__item">
                                                    <a     (click)="openSide($event);$event.stopPropagation();" routerLink="/{{language['LANGUAGE_CODE']}}/collection"
                                                    [queryParams]="{taxons: taxon.code}" (click)="$event.stopPropagation()" class="dropdown__link">{{ taxon.name }}</a>
                                                </li>
                                            </ng-container>
                                            <!--taxon don't have any children-->


                                        </ng-container>
                                    </ul>
                                    <!--drop down first level-->
                                </ng-container>

                            </li>

                            <!--static pages-->
                            <ng-container *ngIf="staticPages?.length">
                                <ng-container *ngFor="let staticPage of staticPages">
                                    <li class="header__primaryMenu__list" >
                                        <a
                                        (click)="openSide($event);$event.stopPropagation();;$event.stopPropagation();"
                                        [routerLink]="['/', language['LANGUAGE_CODE'], 'page', staticPage.code]" 
                                        class="header__primaryMenu__link"
                                         routerLinkActive="active">
                                        {{staticPage | translate:'name'}}
                                        </a>
                                    </li>
                                </ng-container>
                            </ng-container>
                            <!--static pages-->
                          
                        </ul>
                        <!--primary menu-->

                    </div>
                </div>
                <!--blackLayer-->

                <!--secondary menu cart - setting -->
                <ul class="secondary-menu">
                    <li class="secondary-menu__item site-setting" appClosest>
                        <svg class="secondary-menu__icon svg-icon" viewBox="0 0 20 20">
                                <path d="M10.032,8.367c-1.112,0-2.016,0.905-2.016,2.018c0,1.111,0.904,2.014,2.016,2.014c1.111,0,2.014-0.902,2.014-2.014C12.046,9.271,11.143,8.367,10.032,8.367z M10.032,11.336c-0.525,0-0.953-0.427-0.953-0.951c0-0.526,0.427-0.955,0.953-0.955c0.524,0,0.951,0.429,0.951,0.955C10.982,10.909,10.556,11.336,10.032,11.336z"></path>
